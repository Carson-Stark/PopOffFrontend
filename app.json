{
  "expo": {
    "name": "PopOff",
    "slug": "byteverse",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.carsonrs.popoff",
      "buildNumber": "1.1.${EAS_BUILD_NUMBER}",
      "usesNonExemptEncryption": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to let you upload and share videos on the platform.",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save downloaded posts to your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carsonrs.popoff"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-video",
      "expo-secure-store",
      [
        "@config-plugins/ffmpeg-kit-react-native",
        {
          "podspecUrl": "https://raw.githubusercontent.com/luthviar/ffmpeg-kit-ios-full/main/ffmpeg-kit-ios-full.podspec"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0c159784-d6c1-42d0-ba70-b3119017153d"
      }
    },
    "owner": "carsonrs"
  }
}
